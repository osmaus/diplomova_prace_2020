<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mystiq">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".AnswerResultsActivity"
                  android:label="Výsledky podle odpovědí"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="Mystiq">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AttachmentActivity"
            android:label="Přílohy"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".InstancesActivity"
            android:label="Jednotlivé návštěvy"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TotalActivity"
            android:label="Celkové výsledky"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TimeResultsActivity"
            android:label="Výsledky za období"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AboutActivity"
            android:label="O aplikaci"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TotalActivity1"
            android:label="@string/title_activity_total1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>